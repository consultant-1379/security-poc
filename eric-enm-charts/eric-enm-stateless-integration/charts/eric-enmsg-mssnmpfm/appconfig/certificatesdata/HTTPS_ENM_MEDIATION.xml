<?xml version="1.0" encoding="UTF-8"?>
<applications xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="CertificateRequest.xsd">
    <application>
        <certificates>
            <certificate>
                <certificatechain>true</certificatechain>
                <tbscertificate>
                    <subject>
                        <entityname>ENM_HTTPS_MEDIATION_##hostname##</entityname>
                    </subject>
                </tbscertificate>
                <endentityprofilename>ENM_System_SBI_COM_TPFC_EP</endentityprofilename>
                <keypair>
                    <keypairsize>2048</keypairsize>
                    <keypairalgorithm>RSA</keypairalgorithm>
                </keypair>
                <keystore>
                    <jkskeystore>
                        <storealias>HTTPS_ENM_MEDIATION_KEYSTORE</storealias>
                        <storelocation>/ericsson/mediation/data/certs/tlshttpsconf.jks</storelocation>
                        <storepassword>secret</storepassword>
                    </jkskeystore>
                </keystore>
                <truststore>
                    <jkstruststore>
                        <storealias>HTTPS_ENM_MEDIATION_TRUSTSTORE</storealias>
                        <storelocation>/ericsson/mediation/data/certs/tlshttpsconfCA.jks</storelocation>
                        <storepassword>secret</storepassword>
                    </jkstruststore>
                </truststore>
            </certificate>
        </certificates>
        <crlupdatemode>CrlNotManaged</crlupdatemode>
    </application>
    <user>
        <username>root</username>
        <usergroup>root</usergroup>
    </user>
</applications>

