<?xml version="1.0" encoding="UTF-8"?>
<applications xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="CertificateRequest.xsd">
  <application>
    <certificates>
      <certificate>
        <certificatechain>true</certificatechain>
        <tbscertificate>
          <subject>
            <entityname>##hostname##_ENM_NETCONF_MEDIATION</entityname>
          </subject>
	</tbscertificate>
	<endentityprofilename>ENM_System_SBI_COM_TPFC_EK_EP</endentityprofilename>
	<keypair>
	  <keypairsize>2048</keypairsize>
	  <keypairalgorithm>RSA</keypairalgorithm>
	</keypair>
	<keystore>
	  <base64keystore>
	    <storealias>NETCONF_ENM_MEDIATION_KEYSTORE</storealias>
	    <keyfilelocation>/ericsson/mediation/data/certs/tlsnetconf.key</keyfilelocation>
	    <certificatefilelocation>/ericsson/mediation/data/certs/tlsnetconf.cert</certificatefilelocation>
	  </base64keystore>
	</keystore>
	<keystore>
	  <jkskeystore>
	    <storealias>NETCONF_ENM_MEDIATION_KEYSTORE</storealias>
	    <storelocation>/ericsson/mediation/data/certs/tlshttpsconf.jks</storelocation>
	    <storepassword>secret</storepassword>
	  </jkskeystore>
	</keystore>
	<truststore>
	  <base64truststore>
	    <storealias>NETCONF_ENM_MEDIATION_TRUSTSTORE</storealias>
	    <storelocation>/ericsson/mediation/data/certs/tlsnetconfCA.pem</storelocation>
          </base64truststore>
	</truststore>
	<truststore>
	  <jkstruststore>
	    <storealias>NETCONF_ENM_MEDIATION_TRUSTSTORE</storealias>
	    <storelocation>/ericsson/mediation/data/certs/tlshttpsconfCA.jks</storelocation>
	    <storepassword>secret</storepassword>
          </jkstruststore>
	</truststore>
      </certificate>
    </certificates>
    <crlupdatemode>CrlNotManaged</crlupdatemode>
  </application>
  <user>
    <username>root</username>
    <usergroup>root</usergroup>
  </user>
</applications>
