	<TrustProfile Name="ENM PKI Root CA" >
		<InternalCA>CN=ENM PKI Root CA</InternalCA>
	</TrustProfile>	
		

	<TrustProfile Name="ENM Infrastructure CA">
		<InternalCA>CN=ENM PKI Root CA</InternalCA>
		<InternalCA>CN=ENM Infrastructure CA</InternalCA>
	</TrustProfile>	
	
	<TrustProfile Name="ENM OAM CA">
