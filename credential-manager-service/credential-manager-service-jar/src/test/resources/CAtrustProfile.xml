		<InternalCA>CN=ENM Infrastructure CA</InternalCA>
		<InternalCA>CN=ENM OAM CA</InternalCA>
	</TrustProfile>	
	
	<TrustProfile Name="ENM Management CA">
		<InternalCA>CN=ENM PKI Root CA</InternalCA>
		<InternalCA>CN=ENM Infrastructure CA</InternalCA>
		<InternalCA>CN=ENM Management CA</InternalCA>
	</TrustProfile>	
	
	<TrustProfile Name="ENM UI CA">
		<InternalCA>CN=ENM PKI Root CA</InternalCA>
		<InternalCA>CN=ENM Infrastructure CA</InternalCA>
		<InternalCA>CN=ENM UI CA</InternalCA>
	</TrustProfile>	
	
	<TrustProfile Name="ENM NBI CA">
		<InternalCA>CN=ENM PKI Root CA</InternalCA>
		<InternalCA>CN=ENM Infrastructure CA</InternalCA>
		<InternalCA>CN=ENM NBI CA</InternalCA>
	</TrustProfile>	
    
    <TrustProfile Name="EPPKI_TP">
