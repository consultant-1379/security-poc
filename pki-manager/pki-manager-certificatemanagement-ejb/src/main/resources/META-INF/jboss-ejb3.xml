<jboss xmlns="http://www.jboss.com/xml/ns/javaee" xmlns:jee="http://java.sun.com/xml/ns/javaee"
	xmlns:c="urn:clustering:1.0">
	<jee:assembly-descriptor>
		<c:clustering>
			<jee:ejb-name>CACertificateManagementServiceBean</jee:ejb-name>
			<c:clustered>true</c:clustered>
		</c:clustering>
	</jee:assembly-descriptor>

	<jee:assembly-descriptor>
		<c:clustering>
			<jee:ejb-name>EntityCertificateManagementServiceBean</jee:ejb-name>
			<c:clustered>true</c:clustered>
		</c:clustering>
	</jee:assembly-descriptor>
	<jee:assembly-descriptor>
		<c:clustering>
			<jee:ejb-name>ExtCACertificateManagementServiceBean</jee:ejb-name>
			<c:clustered>true</c:clustered>
		</c:clustering>
	</jee:assembly-descriptor>
	<jee:assembly-descriptor>
		<c:clustering>
			<jee:ejb-name>EntityCertificateManagementCustomServiceBean</jee:ejb-name>
			<c:clustered>true</c:clustered>
		</c:clustering>
	</jee:assembly-descriptor>
</jboss>